{
  "name": "www.memefun.xyz",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite --mode dev --host",
    "dev:test": "vite --mode test --host",
    "dev:prod": "vite --mode prod --host",
    "build": "vite --mode prod build",
    "build:dev": "vite --mode dev build",
    "build:test": "vite --mode test build",
    "preview": "vite preview --host",
    "lint": "eslint src --ext .js,.ts",
    "format": "prettier --write src",
    "mirror:get": "npm config get registry",
    "mirror:set": "npm config set registry https://registry.npmmirror.com",
    "mirror:rm": "npm config rm registry",
    "backup": "git add . && git commit -m \"chore: backup\" && git push",
    "up": "npx npm-check-updates -u",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@bassist/progress": "^0.2.1",
    "@bassist/utils": "^0.11.2",
    "@metamask/sdk": "^0.10.0",
    "axios": "^1.6.7",
    "bn.js": "^5.2.1",
    "element-plus": "^2.3.7",
    "events": "^3.3.0",
    "fabric": "^5.3.0",
    "gif.js": "^0.2.0",
    "gifshot": "^0.4.5",
    "gifuct-js": "^2.1.2",
    "masonry-layout": "^4.2.2",
    "pinia": "^2.0.33",
    "unplugin-icons": "^0.16.5",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-observe-visibility": "^1.0.0",
    "vue-router": "^4.1.6",
    "web3": "^4.4.0"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.1.11",
    "@types/fabric": "^5.3.3",
    "@types/gif.js": "^0.2.2",
    "@types/masonry-layout": "^4.2.7",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "less": "^4.1.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.6",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.5.2",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-html": "^3.2.0",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.2.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --concurrent false"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
